<p id="notice"><%= notice %></p>

<h1>Arthicles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arthicles.each do |arthicle| %>
      <tr>
        <td><%= arthicle.title %></td>
        <td><%= arthicle.author %></td>
        <td><%= arthicle.text %></td>
        <td><%= link_to 'Show', arthicle %></td>
        <td><%= link_to 'Edit', edit_arthicle_path(arthicle) %></td>
        <td><%= link_to 'Destroy', arthicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Arthicle', new_arthicle_path %>
